		"live reload",
		"typescript",
		"sass"
	],
	"author": "Clemaister",
	"license": "",
	"main": "electron.js",
	"scripts": {
		"dev": "tsc electron.ts --outDir . && npm-run-all -p start:webpack:mock:server",
		"mock:server": "json-server --watch src/assets/data/db.json",
		"start:webpack": "cross-env NODE_ENV=dev webpack-dev-server --port=4200",
		"start:webpack:mock:server": "concurrently --kill-others \"npm run mock:server\" \"npm run start:webpack\"",
		"compile": "webpack",
		"electron:rebuild": "npm rebuild --runtime=electron --target=1.7.10 --disturl=https://atom.io/download/atom-shell --abi=54",
		"packager": "node packager.js",
		"build": "npm run packager --publish=false",
		"release": "npm run packager --publish=true"
	},
