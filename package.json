{
	"name": "test-fj",
	"version": "1.0.2",
	"description": "Test fj",
	"repository": {
		"type": "git",
		"url": "https://github.com/Clemaister/test-fj"
	},
	"keywords": [
		"electron",
		"angular",
		"webpack",
